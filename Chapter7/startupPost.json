[
  {
    "description": "模拟一个无参post请求",
    "request": {
      "uri": "/postdemo",
      "method": "post"
    },
    "response": {
      "text": "这是一个moco中的post请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "模拟一个带参的post请求",
    "request": {
      "uri": "/postwithparam",
      "method": "post",
      "forms": {
        "name": "xin",
        "sex": "man"
      }
    },
    "response": {
      "text": "这是一个带参的Post请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
